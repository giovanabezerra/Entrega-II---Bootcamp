// O seletor que o teste do Playwright está usando para identificar o link é mais específico.
// Usamos o mesmo seletor aqui para garantir que estamos modificando O MESMO elemento
// que o Playwright está verificando.

const specificLinkSelector = 'a[href="https://www.iana.org/domains/example"]';

// Tenta obter o link específico na página de exemplo.
const link = document.querySelector(specificLinkSelector); 

// Aplica o estilo se o link for encontrado.
if (link) {
    // Aplica um estilo visível (diferente de 'none') para o teste passar.
    link.style.outline = '4px solid red'; 
}
